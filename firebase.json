{
  "hosting": {
    "target": "rebot",
    "public": "build",
    "redirects": [ {
      "source": "/get",
      "destination": "/start.html",
      "type": 301
    } ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "emulators": {
    "firestore": {
      "port": 8080
    },
    "ui": {
      "enabled": true
    }
  }
}
